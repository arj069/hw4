<h1>Places</h1>

<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
</ul>

<p>
  <div class="d-flex justify-content-end mt-3">
    <button type="submit" class="btn btn-primary">
      <a href="/places/new">New place</a>
    </button>
  </div>
</p>